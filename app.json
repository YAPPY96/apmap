{
  "expo": {
    "name": "Koudaisai Map",
    "slug": "university-map",
    "version": "1.10.18",
    "orientation": "portrait",
    "icon": "./assets/image/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "icon": "./assets/image/icon.png",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "このアプリは地図上で現在地を表示するために位置情報を使用します。",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "このアプリは地図上で現在地を表示するために位置情報を使用します。",
  
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "unpkg.com": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            },
            "openstreetmap.org": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "bundleIdentifier": "com.KoudaisaiExecutiveCommittee.Koudaisaimap"
    },
    "android": {
      "icon": "./assets/image/icon.png",
      "package": "com.KoudaisaiExecutiveCommittee.Koudaisaimap",
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/image/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "このアプリは地図上で現在地を表示するために位置情報を使用します。"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "assets/**"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a0d9037b-75d3-4ecd-bdd3-d6b07f53566e"
      }
    },
    "owner": "yappy96"
  }
}

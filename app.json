{
  "expo": {
    "name": "University Map",
    "slug": "university-map",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "permissions": [
      "LOCATION",
      "ACCESS_FINE_LOCATION",
      "ACCESS_COARSE_LOCATION"
    ],
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "このアプリは地図上で現在地を表示するために位置情報を使用します。",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "この アプリは地図上で現在地を表示するために位置情報を使用します。"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    }
  }
}